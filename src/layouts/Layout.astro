---
import Base from "@/layouts/Base.astro";
import { RootLayout } from "@/components/RootLayout";
import "@/styles/tailwind.css";

interface Props {
  title: string;
}

const { title = "DoubleWord Labs" } = Astro.props;
---

<Base title={title} description="" bodyClass="flex min-h-full flex-col">
  <RootLayout client:load>
    <slot />
  </RootLayout>
</Base>
